{
  "name": "react-to-typescript-definitions",
  "version": "1.2.0",
  "description": "Create typescript definitions files (d.ts) from react components",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "react2dts": "cli.js"
  },
  "files": [
    "dist",
    "cli.js",
    "index.d.ts"
  ],
  "scripts": {
    "linter": "tslint --project ./tsconfig.json",
    "start": "npm test",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc --sourceMap",
    "build:watch": "npm run build -- --watch",
    "build:inline": "tsc --inlineSourceMap",
    "pretest": "npm run clean && npm run build:inline",
    "test": "nyc ava",
    "coverage": "nyc report --reporter=lcov && codecov",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w",
    "prerelease": "npm test && npm run build",
    "release": "standard-version",
    "postrelease": "git push --follow-tags origin master && npm publish"
  },
  "author": {
    "name": "Markus Wolf",
    "email": "knister.peter@shadowrun-clan.de"
  },
  "repository": {
    "type": "git",
    "url": "KnisterPeter/react-to-typescript-definitions"
  },
  "license": "MIT",
  "devDependencies": {
    "@knisterpeter/standard-tslint": "1.7.2",
    "@types/babel-generator": "6.25.3",
    "@types/diff": "4.0.2",
    "@types/execa": "0.9.0",
    "@types/node": "9.6.47",
    "@types/prop-types": "15.7.1",
    "@types/react": "16.8.14",
    "ava": "0.25.0",
    "babel-core": "6.26.3",
    "babel-preset-es2015": "6.24.1",
    "babel-register": "6.26.0",
    "chokidar-cli": "1.2.2",
    "codecov": "3.3.0",
    "conventional-changelog-cli": "2.0.17",
    "coveralls": "3.0.3",
    "cz-conventional-changelog": "2.1.0",
    "diff": "4.0.1",
    "execa": "1.0.0",
    "nyc": "13.3.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "rimraf": "2.6.3",
    "source-map-support": "0.5.12",
    "standard-version": "5.0.2",
    "tslint": "5.16.0",
    "typescript": "3.4.5"
  },
  "dependencies": {
    "astq": "2.3.6",
    "babel-generator": "6.26.1",
    "babylon": "7.0.0-beta.47",
    "chalk": "2.4.2",
    "dts-dom": "3.1.1",
    "get-stdin": "7.0.0",
    "meow": "5.0.0",
    "pascal-case": "2.0.1",
    "strip-ansi": "5.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "files": [
      "dist/tests/**/*-test.js"
    ],
    "source": [
      "dist/src/**/*.js"
    ],
    "require": [
      "source-map-support/register"
    ]
  },
  "nyc": {
    "all": true,
    "cache": true,
    "exclude": [
      "wallaby.conf.js",
      "node_modules",
      "coverage",
      "dist/tests",
      "tests",
      "dist/src/deprecated.js",
      "dist/src/analyzer.js"
    ]
  }
}
